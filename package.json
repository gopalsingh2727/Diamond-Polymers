  "private": true,
  "version": "1.0.1",
  "type": "module",
  "appId": "com.27Infinity.27this",
  "repository": {
    "type": "git",
    "url": "git@github.com:gopalsingh2727/Diamond-Polymers.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && electron-builder",
    "build:check": "tsc && vite build && electron-builder",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build:win": "electron-builder --win",
    "publish": "electron-forge publish",
    "make-mac": "electron-forge make --platform darwin --arch arm64",
    "make": "electron-forge make"
  },
  "build": {
    "appId": "com.27Infinity.27this",
    "productName": "DiamondPolymers",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "node_modules/**/*",
      "package.json",
      "bin/ffmpeg.dll"
    ],
    "extraResources": [
      {
        "from": "bin/",
        "to": "bin/",
        "filter": [
          "**/*"
        ]
      }
    ],
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "category": "public.app-category.productivity",
      "icon": "build/icon.icns"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "build/icon.ico"
    },
    "publish": {
      "provider": "github",
      "owner": "gopalsingh2727",
      "repo": "Diamond-Polymers",
